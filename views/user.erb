<html>
  <head></head>
  <body>
    <div>
      <h1 align='center'>Welcome <%= @maker.name%>!</h1>
    </div>

    <div>
      <h2>This is the list of all the peeps</h2>
     <div>
      <% @peeps.reverse_each do |peep| %>
        <div class="w3-container">
          <div class="last_peeps">
            <%= peep.title %>
            <%= peep.time %>
            <%= peep.maker_id %>
            <%= peep.content %>
          </div>
        </div>
      <% end %>
    </div>
    </div>
  </body>
</html>